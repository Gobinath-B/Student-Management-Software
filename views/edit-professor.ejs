<div class="row" style="padding: 20px;">
     <div class="col-5 card" style="margin: 10px;">
    <h2>Co-curricular Certificate</h2>
    <form action="/imageupload" method="post" enctype="multipart/form-data">
      <input type="file" name="image" class="btn btn-light" style="margin-bottom: 10px;">
      <input type="hidden" name="certificateType" value="coCurricular">
      <input type="submit" class="btn btn-primary" value="Upload Co-curricular Certificate">
    </form>
  </div>
  <div class="col-5 card" style="margin: 10px;">
    <h2>Extra-curricular Certificate</h2>
    <form action="/imageupload" method="post" enctype="multipart/form-data">
      <input type="file" name="image" class="btn btn-light", style="margin-bottom: 10px;">
      <input type="hidden" name="certificateType" value="extraCurricular">
      <input type="submit" class="btn btn-primary" value="Upload Extra-curricular Certificate">
    </form>
  </div>

</div>








